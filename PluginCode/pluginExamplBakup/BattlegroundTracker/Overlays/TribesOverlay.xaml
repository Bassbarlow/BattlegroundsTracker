<UserControl x:Class="BattlegroundTracker.TribesOverlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BattlegroundTracker"
             mc:Ignorable="d" 
             d:DesignHeight="1050" d:DesignWidth="350" VerticalAlignment="Center" HorizontalAlignment="Center">
    <Grid x:Name="panel" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0" Width="350" Height="1050">
        <Image x:Name="imgTribe1" Stretch="Fill" Width="350" Height="350" Margin="0,-700,0,0"></Image>
        <Image x:Name="imgTribe2" Stretch="Fill" Width="350" Height="350" Margin="0,-300,0,0"></Image>
        <Image x:Name="imgTribe3" Stretch="Fill" Width="350" Height="350" Margin="0,300,0,0"></Image>
        <Image x:Name="imgTribe4" Stretch="Fill" Width="350" Height="350" Margin="0,700,0,0"></Image>
    </Grid>
</UserControl>
